<?xml version="1.0"?>
<Container version="2">
  <Name>Android-8.0</Name>
  <Repository>ulisses1478/docker-android</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>true</Privileged>
  <Support/>
  <Project/>
  <Overview>Android in docker solution with noVNC supported and video recording. Work way better with Intel CPUs because AMD doesn't an cpu graphics card. So with amd cpus can be unusable.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:6080]</WebUI>
  <TemplateURL/>
  <Icon>https://duckduckgo.com/i/333122ce.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1559173912</DateInstalled>
  <DonateText>Hey buy me some coffee</DonateText>
  <DonateLink>https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&amp;amp;hosted_button_id=PJNVJNW2CPE3G&amp;amp;source=url</DonateLink>
  <Description>Android in docker solution with noVNC supported and video recording. Work way better with Intel CPUs because AMD doesn't have internal graphical chip. So with AMD CPU may be unusable unless it is an APU.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>6080</HostPort>
        <ContainerPort>6080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value>-gpu swiftshader_indirect -accel on</Value>
      <Name>EMULATOR_ARGS</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Port 1" Target="6080" Default="" Mode="tcp" Description="Container Port: 6080" Type="Port" Display="always" Required="false" Mask="false">6080</Config>
</Container>
